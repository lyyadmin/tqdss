.div-table {
	display: table;
	margin-left: auto;
    margin-right: auto;
}

.div-row {
	display: table-row;
}

.div-cell {
	display: table-cell; 
	vertical-align: middle; 
	padding: 6px 5px;
}
.input-group .form-control {
    position: relative;
    z-index: 2;
    float: left;
    width: 122px;
    margin-bottom: 0;
}
.form-control {
    display: block;
    width: 183px;
    height: 30px;
    padding:0px 12px;
    font-size: 14px;
    line-height: 30px;
    color: #333;
    font-weight:bold;
    border: 1px solid #ccc;
    border-radius: 2;
    -webkit-box-shadow: 2px 2px 5px #ddd;
    box-shadow: 2px 2px 5px #ddd;
    -webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
}
.glyphicon-calendar:before{
	content:"";
}
.glyphicon-calendar{
	width:16px;
	height:16px;
    background: url(../images/calendor_icon.png) no-repeat 0 center;
    background-size:16px;
}
.input-append.date .add-on i, .input-prepend.date .add-on i, .input-group.date .input-group-addon span {
    cursor: pointer;
    width: 16px;
    height: 16px;
    padding:0;
}
.input-group .form-control:last-child, .input-group-addon:last-child, .input-group-btn:first-child>.btn-group:not(:first-child)>.btn, .input-group-btn:first-child>.btn:not(:first-child), .input-group-btn:last-child>.btn, .input-group-btn:last-child>.btn-group>.btn, .input-group-btn:last-child>.dropdown-toggle {
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
}
.btn-primary {
    color: #fff;
    background-color: #f39800;
    border-color: #f39800;
    padding: 4px 30px;
    box-shadow: 2px 2px 5px #ddd;
    margin-left: 10px;
}
.btn-primary:hover,
.btn-primary:focus {
    color: #fff;
    background-color: #f39800;
    border-color: #f39800;
    padding: 4px 30px;
}
.input-group-addon {
    padding: 0px 8px;
    font-size: 14px;
    font-weight: 400;
    line-height: 1;
    color: #555;
    text-align: center;
    background-color: #FFF;
    border: 1px solid #ccc;
    border-radius: 0;
    -webkit-box-shadow: 2px 2px 5px #ddd;
    box-shadow: 2px 2px 5px #ddd;
}
.input-group-add {
	width:100px;
    font-size: 14px;
    font-weight: 400;
    color: #000;
    text-align: center;
    background-color: #FFF;
    border: 0;
    border-radius: 4px;
    font-weight: bold;
    float:right;
    text-shadow: 2px 2px 3px #eee;
    text-algin:right;
}
.search_container{
	width:auto;
	margin:10px;
	background:#FFF;
    box-shadow: 3px 3px 3px #ddd;
    border-radius: 5px;
    padding:15px 0;
    border:solid 1px #ddd;
    text-algin:center;
}
.btn-default{
	width: 150px;
    line-height: 30px;
    color: #333;
    font-weight:bold;
    border: 1px solid #ccc;
    border-radius: 0;
    -webkit-box-shadow: 2px 2px 5px #ddd;
    box-shadow: 2px 2px 5px #ddd;
    height: 30px;
    padding:0px 12px;
}
.bootstrap-select.btn-group.show-tick .dropdown-menu li a span.text {
    margin-right: 14px;
}
.bootstrap-select.form-control:not([class*="col-"]) {
    width: 156px;
}
.box-shadow{
	border:solid 1px #ddd;
	box-shadow: 2px 2px 5px #ddd;
	border-radius: 5px;
	background:#fff;
}
body{
	background:#f5f5f5;
}
.title{
	display:inline-block;
	position:relative;
	background:#00F;
	color:#FFF;
	padding: 5px 23px;
    border-radius: 3px;
    font-size:14px;
    margin:10px 10px;
    font-weight:bold;
}
.tenly-btn{
	display:inline-block;
	position:relative;
	background:#00F;
	color:#FFF;
	padding: 2px 23px;
    border-radius: 3px;
    font-size:14px;
    font-weight:bold;
    border:0px;
    outline:none;
    box-shadow: 0 4px 4px 0 rgba(0,0,0,0.2), 0 6px 20px 0 rgba(0,0,0,0.1);
}
.tenly-btn:hover{
	cursor:pointer;
}
.menu-btn{
	list-style:none;
	display:inline-block;
}
.menu-btn li{
	display:inline-block;
	position:relative;
	width:100px;
	height:30px;
	line-height:30px;
	text-align:center;
	background:#FFF;
	float:left;
	padding-bottom:2px;
	color:#333;
	border-bottom:solid 2px #aaa;
	font-size:14px;
}
.menu-btn li:hover{
	cursor:pointer;
}
.menu-btn .selected{
	color:#f39800;
	border-bottom:solid 3px #f39800;
}